        <splash height="480" platform="ios" src="res/screens/ios/Default~iphone.png" width="320" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="Orientation" value="portrait" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova.plugins.diagnostic" spec="^4.0.5" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-request-location-accuracy" spec="^2.2.3" />
    <plugin name="cordova-android-play-services-gradle-release" spec="^2.0.0">
        <variable name="PLAY_SERVICES_VERSION" value="16.+" />
    </plugin>
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-firebase-messaging" spec="^1.2.1">
        <variable name="FIREBASE_CORE_VERSION" value="16.0.+" />
        <variable name="FIREBASE_MESSAGING_VERSION" value="17.3.+" />
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-googleplus" spec="^6.0.0">
        <variable name="WEB_APPLICATION_CLIENT_ID" value="782571423011-if229b9mkrlqc3sfjpgip03232mtl0mq.apps.googleusercontent.com" />
        <variable name="REVERSED_CLIENT_ID" value="reversed" />
    </plugin>
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.1" />
    <engine name="ios" spec="^4.5.5" />
</widget>
